    let params = {\r\n        method : 'POST',\r\n        headers : {\r\n          'Accept': 'application/json',\r\n          'content-type': 'application/JSON'\r\n        },\r\n        body : JSON.stringify({\r\n           name : ahem.name,\r\n           email : ahem.email,\r\n          password : ahem.password \r\n        })\r\n      }\r\n      fetch('https://reqres.in/api/users',params)\r\n      .then(res=>res.json()).then(data=>console.log(data)).catch(()=>console.log('error'))\r\n    }\r\n\r\n    changeHandler(e){\r\n        e.preventDefault()\r\n        const {name,value} = e.target\r\n        let errors = this.state.errors\r\n        switch (name) {\r\n            case 'name': \r\n              errors.fullName = \r\n                value.length < 5\r\n                  ? 'Full Name must be at least 5 characters long!'\r\n                  : '';\r\n              break;\r\n            case 'email': \r\n              errors.email = \r\n                validEmailRegex.test(value)\r\n                  ? ''\r\n                  : 'Email is not valid!';\r\n              break;\r\n            case 'password': \r\n              errors.password = \r\n                value.length < 8\r\n                  ? 'Password must be at least 8 characters long!'\r\n                  : '';\r\n              break;\r\n            default:\r\n              break;\r\n          }\r\n      \r\n          this.setState({errors, [name]: value});\r\n        }\r\n    \r\n    render() {\r\n\r\n        return (\r\n            <div>\r\n                    <Form className = \"form\" autoComplete = \"off\" onSubmit={this.handleSubmit}>\r\n                        <FormGroup>\r\n                            <Form.Control type = \"text\" name = \"name\" placeholder = \"Full Name\" className = \"input\" onChange = {this.changeHandler} value = {this.state.name} id =\"name\"/>\r\n                        </FormGroup>\r\n                        <FormGroup>\r\n                            <Form.Control type = \"email\" name = \"email\" placeholder = \"Email Address\" className = \"input\" onChange = {this.changeHandler} value = {this.state.email}/>\r\n                        </FormGroup>\r\n                        <FormGroup>\r\n                            <Form.Control type = \"password\" name = \"password\" placeholder = \"Choose Password\" className = \"input\" onChange = {this.changeHandler} value = {this.state.password}/>\r\n                        </FormGroup>\r\n                        <FormGroup>\r\n                            <Form.Control type = \"password\" name = \"pass2\" placeholder = \"Confirm Password\" className = \"input\" onChange = {this.changeHandler} value = {this.state.pass2}/>\r\n                        </FormGroup>\r\n                        {this.state.error && <><small style={{ color: 'red' }}>{this.state.error}</small><br /></>}\r\n                        <Button type = \"submit\" disabled = {this.state.loading} variant = \"success\"  onClick = {this.postHandler} style = {{marginTop: '15px', textDecoration: 'none'}} className = \"button\">Make the Account!</Button>\r\n                    </Form>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default RegForm\r\n","C:\\Users\\Himanshu Loona\\Desktop\\T29-Drumsticks\\client-side\\src\\components\\RegComp\\RegNav.js",["81","82","83"],"C:\\Users\\Himanshu Loona\\Desktop\\T29-Drumsticks\\client-side\\src\\components\\HomeComp\\HomeNav.js",["84","85","86"],"C:\\Users\\Himanshu Loona\\Desktop\\T29-Drumsticks\\client-side\\src\\components\\HomeComp\\HomeForm.js",["87"],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":1,"column":8,"nodeType":"94","messageId":"95","endLine":1,"endColumn":13},{"ruleId":"92","severity":1,"message":"96","line":4,"column":48,"nodeType":"94","messageId":"95","endLine":4,"endColumn":52},{"ruleId":"92","severity":1,"message":"97","line":5,"column":8,"nodeType":"94","messageId":"95","endLine"